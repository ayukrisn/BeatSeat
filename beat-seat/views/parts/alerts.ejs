<!-- Alert -->
<div class="col-auto col-lg-12 mt-2">
    <% if(alert.message != "") { %>
        <div class="alert alert-<%= alert.status %>" role="alert">
            <%= alert.message %>
        </div>
    <% } %>
    <% if(error_msg != '') { %>
        <div class="alert alert-danger" role="alert">
            <%= error_msg %> 
        </div>
    <% } %> 
    <% if(error != '') { %>
        <div class="alert alert-danger" role="alert">
            <%= error %>
        </div>
    <% } %>
</div>